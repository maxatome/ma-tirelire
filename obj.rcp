FORM TransListFormIdx=formAuto++ AT (0 0 160 160)
USABLE
MENUID TransListMenu
BEGIN
	TITLE STR(5 + 31 + 15)

	LIST ""	ID TransListDBases AT (80 PREVTOP 30 AUTO) VISIBLEITEMS 1 NONUSABLE
	LIST ""	ID TransListAccounts AT (0 16-1 30 AUTO) VISIBLEITEMS 1 NONUSABLE
	
	BUTTON "%" ID TransListMiniStats AT (RIGHT@160-11-11-9-2 0 10 12) NOFRAME

	FORMBITMAP AT (PREVRIGHT+2 2) BITMAP bmpLock USABLE
	PUSHBUTTON "" ID TransListToggleCheck AT (PREVLEFT-1 PREVTOP-1 9 10)

    BUTTON "" ID TransListGotoFirst AT (PREVRIGHT+1 PREVTOP-1 11 12) NOFRAME
	FORMBITMAP AT (PREVLEFT+2 PREVTOP+1) BITMAP bmpFirstChecked USABLE

    BUTTON "" ID TransListGotoNext AT (PREVLEFT+11-2 PREVTOP-1 11 12) NOFRAME
	FORMBITMAP AT (PREVLEFT+1 PREVTOP+1) BITMAP bmpNextChecked USABLE

	LIST [$Prefs-FirstNextList] ID TransListPrefsFirstNextList
		AT (PREVLEFT PREVTOP AUTOLISTWIDTH AUTO) VISIBLEITEMS 4 NONUSABLE

	TABLE ID SumListTable
		AT (0 16-1 TABLE_WIDTH 12*11+1)
		ROWS 50 COLUMNS 1 COLUMNWIDTHS TABLE_WIDTH

	FORMBITMAP AT (160-7 PREVTOP+1) BITMAP bmpScrollTop USABLE
	BUTTON "" ID SumListScrollTop AT (PREVLEFT PREVTOP-1 7 10) NOFRAME

	SCROLLBAR ID SumListScrollbar
		AT (RIGHT@160 PREVTOP+PREVHEIGHT 7 12*11-(2*PREVHEIGHT))
		VALUE 0 MIN 0 MAX 100 PAGESIZE 12

	FORMBITMAP AT (160-7 PREVTOP+PREVHEIGHT+1) BITMAP bmpScrollBottom USABLE
	BUTTON "" ID SumListScrollBottom AT (PREVLEFT PREVTOP-1 7 10) NOFRAME


	BUTTON "" ID TransListDebit AT (1 BOTTOM@160-1 14 10)
	FORMBITMAP AT (PREVLEFT+3 160-12+2) BITMAP bmpMinus USABLE

    BUTTON "" ID TransListCredit AT (1+14+1 BOTTOM@160-1 14 10)
	FORMBITMAP AT (PREVLEFT+3 160-12+2) BITMAP bmpPlus USABLE


	FORMBITMAP AT (PREVLEFT-3+14+2 160-1-10) BITMAP bmpDateUp USABLE
	REPEATBUTTON "" ID SumListDateUp
		AT (PREVLEFT-1 PREVTOP-1 7 6) NOFRAME
	FORMBITMAP AT (PREVLEFT+1 PREVTOP+8) BITMAP bmpDateDown USABLE
	REPEATBUTTON "" ID SumListDateDown
		AT (PREVLEFT-1 PREVTOP-2 7 6) NOFRAME

	SELECTORTRIGGER "" ID SumListSumTypePopup
		AT (PREVRIGHT+1 PREVTOP-5 AUTO 10)
	LIST [$TransList-Sums] ID SumListSumTypeList
		AT (PREVLEFT BOTTOM@160-1 AUTOLISTWIDTH AUTO) VISIBLEITEMS 10 NONUSABLE

	FIELD ID SumListSum
		AT (RIGHT@160-7 BOTTOM@160 60 AUTO)
		RIGHTALIGN MAXCHARS 13 NONEDITABLE FONT LABEL_FONT

	BUTTON "\003" ID SumListFull
		AT (RIGHT@160 BOTTOM@160 7 11) NOFRAME FONT 4

	LIST [$TransList-ContextMenu] ID TransListActionList
		AT (0 0 AUTOLISTWIDTH AUTO)	VISIBLEITEMS 7 NONUSABLE

	LIST ""	ID TransListDescList AT (0 0 30 AUTO) VISIBLEITEMS 1 NONUSABLE
END

