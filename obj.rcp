FORM OpFormIdx=formAuto++ AT (0 0 160 160)
USABLE
MENUID TransMenu
DEFAULTBTNID OpCancel
BEGIN
	TITLE STR(16)

DEFINE OpOutOfTab=baseTabOut

	BUTTON "X" ID OpPrev=OpOutOfTab++ AT (60 0 13 12) NOFRAME FONT 5
	BUTTON "X" ID OpNext=OpOutOfTab++ AT (PREVRIGHT 0 13 12) NOFRAME FONT 5

	SELECTORTRIGGER STR(16) ID OpCategoryPopup=OpOutOfTab++
		AT (RIGHT@159 1 AUTO 11) RIGHTANCHOR
	LIST "" ID OpCategoryList=OpOutOfTab++
		AT (156 0 4 AUTO) VISIBLEITEMS 1 NONUSABLE

	LABEL "$Trans-Date"	ID OpOutOfTab++	AT (RIGHT@XLEFT YTOP-1) FONT LABEL_FONT

	FORMBITMAP AT (PREVRIGHT YTOP-1) BITMAP bmpDateUp USABLE
	REPEATBUTTON "" ID OpDateUp=OpOutOfTab++
		AT (PREVLEFT-1 PREVTOP-1 7 6) NOFRAME
	FORMBITMAP AT (PREVLEFT+1 YTOP-1+9) BITMAP bmpDateDown USABLE
	REPEATBUTTON "" ID OpDateDown=OpOutOfTab++
		AT (PREVLEFT-1 PREVTOP-2 7 6) NOFRAME
	SELECTORTRIGGER STR(14) ID OpDate=OpOutOfTab++
		AT (PREVLEFT+PREVWIDTH+1 YTOP-1 DATE_REC_WIDTH AUTO) LEFTANCHOR

	SELECTORTRIGGER STR(8) ID OpTime=OpOutOfTab++
		AT (PREVLEFT+PREVWIDTH+2 PREVTOP AUTO AUTO) LEFTANCHOR
	PUSHBUTTON "\024" ID OpAlarm=OpOutOfTab++ AT (2 PREVTOP 9 AUTO) FONT 3

	POPUPTRIGGER "$Trans-Desc" ID OpDescPopup=OpOutOfTab++
		AT (RIGHT@XLEFT_POPUP-6 PREVTOP+LINEFEED AUTO AUTO)
		LEFTANCHOR FONT LABEL_FONT
	LIST "" ID OpDescList=OpOutOfTab++
		AT (PREVLEFT PREVTOP 10 AUTO) VISIBLEITEMS 1 NONUSABLE

	LABEL ":" ID OpOutOfTab++ AT (RIGHT@XLEFT PREVTOP) FONT LABEL_FONT

	FIELD ID OpDesc=OpOutOfTab++
		AT (XRIGHT PREVTOP 98 2*11+1)
		UNDERLINED MAXCHARS NOTE_MAXCHARS MULTIPLELINES AUTOSHIFT HASSCROLLBAR
	SCROLLBAR ID OpScrollbar=OpOutOfTab++
		AT (RIGHT@160 PREVTOP 7 PREVHEIGHT)	VALUE 0 MIN 0 MAX 100 PAGESIZE 12

	BUTTON STR(16) ID OpDebCredLabel=OpOutOfTab++
		AT (0 PREVTOP+PREVHEIGHT RIGHT@XLEFT+2 AUTO)
		RIGHTANCHOR NOFRAME FONT LABEL_FONT

	SELECTORTRIGGER "+" ID OpDebCredSign=OpOutOfTab++
		AT (XRIGHT PREVTOP+1 AUTO AMOUNT_SIGN_HEIGHT-1) RIGHTANCHOR
	FIELD ID OpAmount=OpOutOfTab++
		AT (PREVRIGHT+3 PREVTOP-1 AMOUNT_WIDTH AUTO)
		UNDERLINED MAXCHARS AMOUNT_LEN RIGHTALIGN NUMERIC
	
	SELECTORTRIGGER "" ID OpDevise=OpOutOfTab++
		AT (PREVRIGHT+3 PREVTOP+1 AUTO AMOUNT_SIGN_HEIGHT)
    LIST "" ID OpDeviseList=OpOutOfTab++
		AT (PREVLEFT PREVTOP 10 AUTO) VISIBLEITEMS 1 NONUSABLE

DEFINE OpTab1=baseTab1Begin
	BUTTON "$Trans-MainTab" ID baseTab1
		AT (XTAB PREVTOP+LINEFEED AUTOWIDTH+5 HTAB) NOFRAME

DEFINE OpTab2=baseTab2
	BUTTON "$Trans-RepeatTab" ID OpTab2++
		AT (PREVRIGHT+1 PREVTOP AUTOWIDTH+5 HTAB) NOFRAME

DEFINE OpTab3=baseTab3
	BUTTON "$Trans-BreakdownTab" ID OpTab3++
		AT (PREVRIGHT+1 PREVTOP AUTOWIDTH+5 HTAB) NOFRAME

DEFINE OpTab4=baseTab4
	BUTTON "$Trans-CurrencyTab" ID OpTab4++
		AT (PREVRIGHT+1 PREVTOP AUTOWIDTH+5 HTAB) NOFRAME

	/***********************************************************************
     * Main tab
     */
    LABEL "$Trans-Mode" ID OpTab1++	AT (RIGHT@XLEFT 80) FONT LABEL_FONT

    POPUPTRIGGER "" ID OpModePopup=OpTab1++
		AT (XRIGHT-3 PREVTOP 50 AUTO) LEFTANCHOR
    LIST "" ID OpModeList=OpTab1++
		AT (PREVLEFT PREVTOP 10 AUTO) VISIBLEITEMS 1 NONUSABLE

	SELECTORTRIGGER "$Trans-ChkNum" ID OpCheckNumAuto=OpTab1++
		AT (3 PREVTOP+LINEFEED AUTOWIDTH+5 AUTO) LEFTANCHOR
	FIELD ID OpCheckNum=OpTab1++
		AT (PREVLEFT+PREVWIDTH+2 PREVTOP INT_WIDTH AUTO)
		UNDERLINED MAXCHARS INT_LEN RIGHTALIGN NUMERIC

	LABEL "$Trans-OnValueDate" ID OpTab1++
		AT (PREVLEFT+PREVWIDTH+[$Trans-OnValueDate-Add]-5 PREVTOP)

	FORMBITMAP AT (PREVRIGHT+1 PREVTOP) BITMAP bmpDateUp USABLE
	GADGET ID OpValueDateUpBmp=OpTab1++ AT (PREVLEFT PREVTOP 5 5) NONUSABLE
	FORMBITMAP AT (PREVLEFT PREVTOP+9) BITMAP bmpDateDown USABLE
	GADGET ID OpValueDateDownBmp=OpTab1++ AT (PREVLEFT PREVTOP 5 5) NONUSABLE

	REPEATBUTTON "" ID OpValueDateUp=OpTab1++
		AT (PREVLEFT-1 PREVTOP-10 7 6) NOFRAME
	REPEATBUTTON "" ID OpValueDateDown=OpTab1++
		AT (PREVLEFT PREVTOP+8 7 6) NOFRAME
	SELECTORTRIGGER STR(14) ID OpValueDate=OpTab1++
		AT (PREVLEFT+PREVWIDTH+1 PREVTOP-7 DATE_REC_WIDTH AUTO) LEFTANCHOR

	LABEL "$Trans-Type" ID OpTab1++
		AT (RIGHT@XLEFT PREVTOP+LINEFEED) FONT LABEL_FONT

	CHECKBOX "" ID OpPerso=OpTab1++ AT (0 PREVTOP AUTO AUTO)

    POPUPTRIGGER "" ID OpTypePopup=OpTab1++
		AT (XRIGHT-3 PREVTOP 160-(XRIGHT-3) AUTO) LEFTANCHOR
    LIST "" ID OpTypeList=OpTab1++
		AT (PREVLEFT PREVTOP 10 AUTO) VISIBLEITEMS 1 NONUSABLE

	LABEL "$Trans-Statement" ID OpTab1++
		AT (RIGHT@XLEFT PREVTOP+LINEFEED) FONT LABEL_FONT
	FIELD ID OpStatementNum=OpTab1++
		AT (XRIGHT PREVTOP INT_WIDTH AUTO)
		UNDERLINED MAXCHARS INT_LEN RIGHTALIGN NUMERIC

	CHECKBOX "$Trans-Clear" ID OpChecked=OpTab1++
		AT (PREVRIGHT+2 PREVTOP AUTO AUTO)

	LABEL "$Trans-Xfer" ID OpTab1++
		AT (RIGHT@XLEFT PREVTOP+LINEFEED) FONT LABEL_FONT

	SELECTORTRIGGER STR(32) ID OpXferPopup=OpTab1++
		AT (XRIGHT PREVTOP AUTO AUTO) LEFTANCHOR
	LIST "" ID OpXferList=OpTab1++
		AT (PREVLEFT PREVTOP 75 AUTO) VISIBLEITEMS 1 NONUSABLE

	FORMBITMAP AT (160-10 PREVTOP+1) BITMAP bmpTrash USABLE
	SELECTORTRIGGER "" ID OpXferDel=OpOutOfTab++
		AT (RIGHT@159 PREVTOP-1 10 AUTO)


	/***********************************************************************
     * Repeat tab
     */
    LABEL "$Trans-Repeat" ID OpTab2++ AT (RIGHT@XLEFT 80) FONT LABEL_FONT

	POPUPTRIGGER "" ID OpRepeatPopup=OpTab2++
